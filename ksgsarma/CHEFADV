#include <stdio.h>
void func();
int main() 
{
  int a;
  scanf("%d", &a);
  while(a > 0)
  {
    func();
    a--;
  }
  return 0;
}
void func()
{
  long long int n, m, x, y, a, b, c, d;
  scanf("%lld", &n);
  scanf("%lld", &m);
  scanf("%lld", &x);
  scanf("%lld", &y);
  a = n - 1;
  b = m - 1;
  c = n - 2;
  d = m - 2;
  
  if((a % x == 0 && b % y == 0 && a >= 0 && b >= 0) || (c % x== 0 && d % y == 0 && c >= 0 && d >= 0))
  {
    printf("Chefirnemo\n");
  } 
  else
    printf("Pofik\n");  
  }
