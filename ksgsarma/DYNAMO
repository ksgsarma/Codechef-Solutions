#include <stdio.h>
#include <math.h>
void func();
int main()
{
	int t;
	scanf("%d", &t);
	while(t > 0)
	{
		func();
		t--;
	}
	return 0;
}
void func()
{
	long long int n, a, b, c, d, e, x, s;
	scanf("%lld", &n);
	scanf("%lld", &a);
	x = pow(10, n);
	s = 2 * x + a;
	printf("%lld\n", s);
	fflush(stdout);
	scanf("%lld", &b);
	c = x - b;
	printf("%lld\n", c);
	fflush(stdout);
	scanf("%lld", &d);
	e = x - d;
	printf("%lld\n", e);
	fflush(stdout);
	scanf("%lld", &n);
}
